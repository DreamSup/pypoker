#!/home/plutoo/projects/pypoker/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'PyPokerGUI==0.0.7','console_scripts','pypokergui'
__requires__ = 'PyPokerGUI==0.0.7'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('PyPokerGUI==0.0.7', 'console_scripts', 'pypokergui')()
    )
